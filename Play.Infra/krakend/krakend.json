{
  "version": 2,
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "port": 8080,
  "endpoints": [
    {
      "endpoint": "/catalog/items",
      "method": "GET",
      "headers_to_pass": [ "Content-Type" ],
      "backend": [
        {
          "host": [
            "http://host.docker.internal:5000"
          ],
          "url_pattern": "/items",
          "method": "GET",
          "is_collection": "true",
          "extra_config": {},
          "sd": "static",
          "encoding": "json"
        }
      ],
      "extra_config": {},
      "output_encoding": "json",
      "concurrent_calls": 1
    },
    {
      "endpoint": "/catalog/additems",
      "method": "POST",
      "headers_to_pass": [ "Content-Type" ],
      "backend": [
        {
          "host": [
            "http://host.docker.internal:5000"
          ],
          "url_pattern": "/items",
          "method": "POST",
          "is_collection": "false",
          "extra_config": {},
          "sd": "static",
          "encoding": "json"
        }
      ],
      "extra_config": {},
      "output_encoding": "json",
      "concurrent_calls": 1
    },
    {
      "endpoint": "/catalog/getitembyid/{id}",
      "method": "GET",
      "headers_to_pass": [ "Content-Type" ],
      "backend": [
        {
          "host": [
            "http://host.docker.internal:5000"
          ],
          "url_pattern": "/items/{id}",
          "method": "GET",
          "is_collection": "false",
          "extra_config": {},
          "sd": "static",
          "encoding": "json"
        }
      ],
      "extra_config": {},
      "output_encoding": "json",
      "concurrent_calls": 1
    },
    {
      "endpoint": "/catalog/updateitembyid/{id}",
      "method": "PUT",
      "headers_to_pass": [ "Content-Type" ],
      "backend": [
        {
          "host": [
            "http://host.docker.internal:5000"
          ],
          "url_pattern": "/items/{id}",
          "method": "PUT",
          "is_collection": "false",
          "extra_config": {},
          "sd": "static",
          "encoding": "json"
        }
      ],
      "extra_config": {},
      "output_encoding": "json",
      "concurrent_calls": 1
    },
    {
      "endpoint": "/catalog/deleteitembyid/{id}",
      "method": "DELETE",
      "headers_to_pass": [ "Content-Type" ],
      "backend": [
        {
          "host": [
            "http://host.docker.internal:5000"
          ],
          "url_pattern": "/items/{id}",
          "method": "DELETE",
          "is_collection": "false",
          "extra_config": {},
          "sd": "static",
          "encoding": "json"
        }
      ],
      "extra_config": {},
      "output_encoding": "json",
      "concurrent_calls": 1
    },
    {
      "endpoint": "/inventory/getitems",
      "querystring_params": [
        "userId"
      ],
      "method": "GET",
      "headers_to_pass": [ "Content-Type" ],
      "backend": [
        {
          "host": [
            "http://host.docker.internal:5004"
          ],
          "url_pattern": "/items",
          "method": "GET",
          "is_collection": "true",
          "extra_config": {},
          "sd": "static",
          "encoding": "json"
        }
      ],
      "extra_config": {},
      "output_encoding": "json",
      "concurrent_calls": 1
    },
    {
      "endpoint": "/inventory/postitems",
      "method": "POST",
      "headers_to_pass": [ "Content-Type" ],
      "backend": [
        {
          "host": [
            "http://host.docker.internal:5004"
          ],
          "url_pattern": "/items",
          "method": "POST",
          "is_collection": "false",
          "extra_config": {},
          "sd": "static",
          "output_encoding": "no-op",
          "encoding": "no-op"
        }
      ],
      "extra_config": {},
      "output_encoding": "json",
      "concurrent_calls": 1
    }
  ],
  "extra_config": {
    "github_com/devopsfaith/krakend-cors": {
      "allow_methods": [
        "GET",
        "HEAD",
        "POST",
        "PUT",
        "DELETE",
        "CONNECT",
        "OPTIONS",
        "TRACE",
        "PATCH"
      ]
    }
  },
  "output_encoding": "json"
}
